<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="jgamebase.db.model.Publisher" table="PUBLISHERS" schema="APP">
        <id name="id" type="integer">
            <column name="PU_ID" index="PUBLISHERS_PU_ID"/>
            <generator class="increment" />
        </id>
        <property name="name" type="string">
            <column name="PUBLISHER" length="255" index="PUBLISHERS_PUBLISHER"/>
        </property>
    </class>
</hibernate-mapping>
