<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="jgamebase.db.model.Year" table="YEARS" schema="APP">
        <id name="id" type="integer">
            <column name="YE_ID" index="YEARS_YE_ID"/>
            <generator class="increment" />
        </id>
        <property name="nameId" type="integer">
            <column name="YEARVAL" />
        </property>
    </class>
</hibernate-mapping>
