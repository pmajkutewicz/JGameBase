<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="jgamebase.db.model.Artist" table="ARTISTS" schema="APP">
        <id name="id" type="integer">
            <column name="AR_ID" index="ARTISTS_AR_ID"/>
            <generator class="increment" />
        </id>
        <property name="name" type="string">
            <column name="ARTIST" length="255" default="''" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
